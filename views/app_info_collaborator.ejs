<div style="margin-top: 50px;margin-left:20%; width:60%">
    <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Authentication</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><%= owner.username %></td>
        <td><%= owner.email %></td>
        <td>owner</td>
      </tr>
        <% for(var i=0;i<collaborators.length;i++){ %>
        <tr>
            <td><%= collaborators[i].username %></td>
            <td><%= collaborators[i].email %></td>
            <td>collaborator</td>
        </tr>
        <% } %>
    </tbody>
  </table>
  <h4 style="margin-top: 50px">Add New Collaborator</h4>
  <form>
    <div class="form-group row">
      <label class="col-sm-3 form-control-label">Email Address</label>
      <div class="col-sm-7" id="mutipleEmail">
        <input type="email" class="form-control" id="inputTeamMember1">
      </div>
    </div>
    <div class="form-group row">
      <div class="col-sm-2">
        <button type="submit" class="btn btn-secondary">Add</button>
      </div>
      <div class="col-sm-2">
        <button type="reset" class="btn btn-secondary">Reset</button>
      </div>
    </div>
  </form>
</div>
